.btn
  font-size: 18px
  padding 3px 13px
  display inline-block
  position relative
  background radial-gradient(top center,cover,#ffffff, #eef5d4)
  border-radius 25px
  border 3px solid #d2f493
  box-shadow inset 0 -12px 3px 5px #cfde9b

  &:link, &:visited
    color #004b01
    text-decoration none

